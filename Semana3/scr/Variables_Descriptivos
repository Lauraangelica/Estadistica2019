Ancho<-rnorm(n = 32,mean = 10,sd = 1)
Ancho

Largo <- rnorm(n = 32,mean = 15,sd = 1)
Largo

medidas<-cbind(Ancho, Largo)
medidas

Tabla_medidas <- as.data.frame(cbind(paste('spp',1:32,sep = '_'),medidas))
Tabla_medidas

colnames(Tabla_medidas) <- c('Individuos', 'Ancho','Largo')
Tabla_medidas

prp <- Ancho/Largo
prp

